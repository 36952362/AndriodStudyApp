<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jupiter.myfirstandriodapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jupiter.AndriodStudyApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jupiter.AndriodStudyApp.launchanotheractivity.DisplayMessageActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.launchanotheractivity.LaunchAnotherActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.fragmentdemo.FragmentDemoActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.SavingDataDemoActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.keyvaluesets.SavingKeyValueSetsActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.keyvaluesets.SavingKeyValueSetsDisplayActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.savingfiles.internalstorage.SavingOnInternalStorageActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.savingfiles.SavingFileActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.savingfiles.internalstorage.SavingOnInternalFileActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.savingfiles.internalstorage.SavingOnCacheFileActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.savingfiles.externalstorage.SavingOnExternalStoargeActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.savingdatabases.SavingDatabaseActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.savingdata.savingdatabases.DatabaseDisplayActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.interactingwithotherapps.InteractingWithOtherAppsActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.interactingwithotherapps.sendingusertoanotherapps.SendingUserToAnotherAppActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.interactingwithotherapps.getingresultfromotherapps.GettingResultFromOtherAppActivity" />
        <activity android:name="com.jupiter.AndriodStudyApp.allowotherappstartthisactivity.AllowOtherAppStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEND"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="text/plain"/>
                <data android:mimeType="image/*"/>
            </intent-filter>
        </activity>
    </application>

</manifest>